<div class="container">

<%= form_for @task do |f| %>
  <%= f.label :title %>:
  <%= f.text_field :title %><br />
  <%= f.label :details %>:
  <%= f.text_field :details %><br />
  <%= f.check_box :completed %>
  <%= f.label :completed, value: "Completed" %>
  <p><%= f.submit %></p>
<% end %>

</div>




